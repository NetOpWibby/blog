{
  "author": {
    "email": "paul@inc.sh",
    "name": "Paul Anthony Webb"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "app-root-path": "^2.1.0",
    "chewit": "^2.0.2",
    "choo": "^6.13.1",
    "choo-async": "^0.1.1",
    "choo-devtools": "^2.5.1",
    "choo-ssr": "^0.2.1",
    "colorette": "^1.0.7",
    "dedent": "^0.7.0",
    "fastify": "2.0.0-rc.1",
    "fastify-compress": "^0.7.1",
    "fastify-helmet": "^3.0.0",
    "fastify-static": "^2.0.0",
    "fs-extra": "^7.0.1",
    "glob": "^7.1.3",
    "graceful-fs": "^4.1.15",
    "js-yaml": "^3.12.0",
    "marked": "^0.5.2",
    "raw-body": "^2.3.3",
    "xtend": "^4.0.1"
  },
  "description": "Official blog of Paul Anthony Webb",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-external-helpers": "7.2.0",
    "@babel/plugin-proposal-class-properties": "7.2.3",
    "@babel/plugin-proposal-decorators": "7.2.3",
    "@babel/plugin-proposal-export-namespace-from": "7.2.0",
    "@babel/plugin-proposal-function-sent": "7.2.0",
    "@babel/plugin-proposal-json-strings": "7.2.0",
    "@babel/plugin-proposal-numeric-separator": "7.2.0",
    "@babel/plugin-proposal-throw-expressions": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-import-meta": "7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "@inc/eslint-config": "^1.1.2",
    "@inc/sasslint-config": "^1.1.2",
    "eslint": "^5.11.1",
    "husky": "^1.3.1",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^2.5.0",
    "sass": "^1.15.2",
    "sass-lint": "^1.12.1",
    "snazzy": "^8.0.0",
    "standardx": "^3.0.1",
    "updates": "^6.0.3"
  },
  "engines": {
    "node": ">=10.3.0"
  },
  "homepage": "https://inc.sh",
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run test:sass && git add -A :/"
    }
  },
  "main": "server.js",
  "name": "theWebb.blog",
  "private": true,
  "scripts": {
    "css": "sass --update app/sass:app/dist --style compressed",
    "deploy": "vagrant push",
    "format": "eslint '**/*.js' --fix",
    "start": "npm i && npm run css && NODE_ENV=production node server",
    "test": "run-s test:*",
    "test:dependencies": "updates --update ./ --exclude fastify",
    "test:lint": "standardx --verbose | snazzy",
    "test:sass": "sass-lint --config ./node_modules/@inc/sasslint-config/config.json --verbose --no-exit",
    "watch": "run-p watch:*",
    "watch:sass": "sass --watch app/sass:app/dist --style compressed",
    "watch:server": "NODE_ENV=development nodemon server"
  },
  "standardx": {
    "ignore": [
      "app/dist"
    ]
  },
  "version": "2.0.0"
}
